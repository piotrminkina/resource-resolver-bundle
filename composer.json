{
    "name": "piotrminkina/resource-resolver-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Symfony2 Resource Resolver Bundle that resolves dependencies from controllers actions by inject configured resources",
    "authors": [
        {
            "name": "Piotr Minkina",
            "email": "projekty@piotrminkina.pl",
            "homepage": "http://www.piotrminkina.pl"
        }
    ],
    "autoload": {
        "psr-0": { "PMD\\Bundle\\Resource\\ResolverBundle": "" }
    },
    "require": {
        "php": ">=5.3.3",
        "doctrine/inflector": "~1.0",
        "symfony/config": "~2.5",
        "symfony/dependency-injection": "~2.5",
        "symfony/event-dispatcher": "~2.5",
        "symfony/expression-language": "~2.5",
        "symfony/http-kernel": "~2.5",
        "symfony/http-foundation": "~2.5"
    },
    "replace": {
        "piotrminkina/resources-resolver-bundle": "self.version"
    },
    "config": {
        "bin-dir": "bin"
    },
    "target-dir": "PMD/Bundle/Resource/ResolverBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
