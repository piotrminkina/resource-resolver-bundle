<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="pmd_resources_resolver.resources_resolver.class">PMD\ResourcesResolverBundle\ResourcesResolver</parameter>
    </parameters>

    <services>
        <service id="pmd_resources_resolver.resources_resolver"
                 class="%pmd_resources_resolver.resources_resolver.class%">
            <argument type="service" id="pmd_resources_resolver.provider.provider"/>
        </service>
    </services>

</container>
